
class HelloWord {
  var hello: Number
  var world: Number
  var _privateNum = 99

  fun toString: String {
    return "hello" + hello.toString() + "world" + world.toString()
  }

  fun getPrivate: Number {
    return _privateNum
  }
}

// 程序入口
fun main {
  var m: HelloWord = {"hello": 42, "world": 103}

  Console.print(m.getPrivate())
}
