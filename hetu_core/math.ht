num max(num a, num b) {
  if (a > b) return a;
  return b;
}

num min(num a, num b) {
  if (a < b) return a;
  return b;
}

num abs(num x) {
  if (x < 0) return -x;
  return x;
}

external num random();

external num randomInt(num max);

external num sqrt(num x);

external num log(num x);

external num sin(num x);

external num cos(num x);
