fun max(a: Number, b: Number): Number {
  if (a > b) return a
  return b
}

fun min(a: Number, b: Number): Number {
  if (a < b) return a
  return b
}

fun abs(x: Number): Number {
  if (x < 0) return -x
  return x
}

external fun random(): Number

external fun randomInt(max: Number): Number

external fun sqrt(x: Number): Number

external fun log(x: Number): Number

external fun sin(x: Number): Number

external fun cos(x: Number): Number
