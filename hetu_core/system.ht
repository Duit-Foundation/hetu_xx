class System {
  // interpret a command line string
  external static func evalc(command: String): dynamic

  // invoke a global or static member function
  //
  // for member function, func_name should be: "className.FuncName"
  external static func invoke(func_name: String, className: String, args: List): dynamic

  external static func readfile(filepath: String): String

  external static func now(): num
}